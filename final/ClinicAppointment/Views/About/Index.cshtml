@{
    ViewData["Title"] = "About Us";
}

<div class="container mt-5">
    <div class="row">
        <div class="col-lg-12 text-center">
            <h1 class="display-4">About Our Clinic</h1>
            <p class="lead">Providing quality healthcare with easy appointment scheduling.</p>
        </div>
    </div>

    <div class="row mt-4 justify-content-center text-center">
        <div class="col-lg-6 ">
            <img src="data:image/jpeg;base64," class="img-fluid rounded shadow" alt="Clinic Image">
        </div>
        <div class="col-lg-6 d-flex align-items-center mt-4">
            <p class="text-muted">
                Our clinic is dedicated to providing top-notch healthcare services. With experienced doctors and a streamlined appointment booking system, we ensure that you get the best medical attention without the hassle of long waiting times.
            </p>
        </div>
    </div>

    <div class="row mt-5">
        <div class="col-lg-4 text-center">
            <i class="fas fa-user-md fa-3x text-primary"></i>
            <h4 class="mt-3">Experienced Doctors</h4>
            <p>Our team of certified medical professionals is here to assist you.</p>
        </div>
        <div class="col-lg-4 text-center">
            <i class="fas fa-calendar-check fa-3x text-success"></i>
            <h4 class="mt-3">Easy Appointments</h4>
            <p>Book appointments seamlessly with our online system.</p>
        </div>
        <div class="col-lg-4 text-center">
            <i class="fas fa-heartbeat fa-3x text-danger"></i>
            <h4 class="mt-3">Quality Care</h4>
            <p>Your health and well-being are our top priority.</p>
        </div>
    </div>

    <div class="row mt-5">
        <div class="col-lg-12 text-center">
            <h3>Contact Us</h3>
            <p>Email: support@clinicappointment.ca | Phone: (123) 456-7890</p>
        </div>
    </div>
</div>
